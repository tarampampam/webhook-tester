{
  "name": "web",
  "private": true,
  "type": "module",
  "scripts": {
    "generate": "node --no-deprecation api.generate.js ./../api/openapi.yml ./src/api/schema.gen.ts",
    "fmt": "prettier --write ./*.{js,ts} ./src && npm run lint:es -- --fix",
    "lint": "npm run lint:ts && npm run lint:es",
    "lint:ts": "tsc --noEmit",
    "lint:es": "eslint ./src/**/*.{ts,tsx}",
    "test": "vitest --run",
    "serve": "vite dev",
    "watch": "vite build --watch",
    "build": "tsc --noEmit && vite build"
  },
  "dependencies": {
    "@mantine/code-highlight": "^8.2.8",
    "@mantine/core": "^8.2.8",
    "@mantine/hooks": "^8.2.8",
    "@mantine/notifications": "^8.2.8",
    "@tabler/icons-react": "^3.34.1",
    "dayjs": "^1.11.18",
    "dexie": "^4.2.0",
    "dexie-react-hooks": "^4.2.0",
    "highlight.js": "^11.11.1",
    "openapi-fetch": "^0.14.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.8.2",
    "semver": "^7.7.2"
  },
  "devDependencies": {
    "@eslint/compat": "^1.4.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.36.0",
    "@fetch-mock/vitest": "^0.2.15",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@types/semver": "^7.7.1",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.45.0",
    "@vitejs/plugin-react": "^5.0.4",
    "eslint": "^9.36.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "jsdom": "^26.1.0",
    "openapi-typescript": "^7.9.1",
    "postcss": "^8.5.6",
    "postcss-preset-mantine": "^1.18.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "vite": "^7.1.7",
    "vitest": "^3.2.4"
  },
  "volta": {
    "node": "24"
  },
  "engines": {
    "node": ">=24"
  }
